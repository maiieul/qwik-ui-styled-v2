{
  "name": "qwik-ui",
  "description": "Blank project with routing included",
  "engines": {
    "node": ">=22.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "test": "vitest run",
    "test.coverage": "vitest run --coverage.enabled true --coverage.provider=istanbul",
    "test.ui": "vitest --ui"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@floating-ui/dom": "^1.7.4",
    "@iconify/utils": "^3.0.2",
    "@oddbird/popover-polyfill": "^0.6.1",
    "@qds.dev/tools": "0.6.2",
    "@qds.dev/ui": "0.6.2",
    "@qwik-ui/styled": "^0.4.1",
    "@qwik-ui/utils": "^0.3.3",
    "@qwik.dev/core": "2.0.0-beta.15",
    "@qwik.dev/router": "2.0.0-beta.15",
    "@shikijs/rehype": "^3.13.0",
    "@shikijs/transformers": "^3.13.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/css-tree": "^2.3.11",
    "@types/node": "20.19.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-istanbul": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "class-variance-authority": "0.7.1",
    "clipboard-copy": "^4.0.1",
    "css-tree": "^3.1.0",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "2.0.0-beta.11",
    "globals": "16.3.0",
    "maiieul-qwik-v2-lib": "^0.0.8",
    "nx": "^21.6.5",
    "oxc-parser": "^0.94.0",
    "oxc-walker": "^0.5.2",
    "playwright": "^1.57.0",
    "prettier": "3.6.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "qwik-ui": "^0.4.3",
    "rollup": "^4.52.4",
    "shiki": "^3.13.0",
    "specificity": "^1.0.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "1.3.4",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "vite": "7.1.7",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^4.0.15",
    "vitest-browser-qwik": "^0.1.0"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "nx": {}
}
