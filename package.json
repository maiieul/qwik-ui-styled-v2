    "@qwik-ui/styled": "^0.4.1",
    "@qwik-ui/utils": "^0.3.3",
    "@qwik.dev/core": "link:../qwik-v2/packages/qwik",
    "@qwik.dev/router": "link:../qwik-v2/packages/qwik-router",
    "@shikijs/rehype": "^3.13.0",
    "@shikijs/transformers": "^3.13.0",
    "@tailwindcss/vite": "^4.1.14",
